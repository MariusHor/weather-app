{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "": "./src",
          "assets": "./src/assets",
          "constants": "./src/constants",
          "core": "./src/core",
          "styles": "./src/styles",
          "templates": "./src/templates",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
