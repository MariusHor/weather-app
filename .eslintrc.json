{
  "rules": {
  },
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "import/extensions": "off"
      }
    }
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
},
  "plugins": [
    "prettier"
],
"extends": [
  "airbnb-base",
  "prettier"
],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"],
        "moduleDirectory": ["src", "node_modules"]
      },
      "alias": {
        "extensions": [".js", ".jsx"],
        "map": [
          ["", "./src"],
          ["@assets", "./src/assets"],
          ["@components", "./src/components"],
          ["@layouts", "./src/layouts"],
          ["@constants", "./src/constants"],
          ["@styles", "./src/styles"],
          ["@utils", "./src/utils"]
        ]
      }
    }
  }
}